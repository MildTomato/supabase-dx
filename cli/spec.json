          "deprecated": false,
          "description": "Show what would be synced without applying"
        },
        {
          "name": "verbose",
          "shorthand": "v",
          "deprecated": false,
          "description": "Show detailed pg-delta logging"
        },
        {
          "name": "json",
          "shorthand": null,
          "deprecated": false,
          "description": "Output as JSON (events as newline-delimited JSON)"
        }

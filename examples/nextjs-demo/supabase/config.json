{
  "$schema": "../../../cli/config-schema/config.schema.json",
  "project_id": "dumltzfoaxseaekszcnt",
  "api": {
    "schemas": [
      "public",
      "graphql_public",
      "storage"
    ],
    "extra_search_path": [
      "public",
      "extensions"
    ],
    "max_rows": 1000
  },
  "auth": {
    "site_url": "https://myapp.com",
    "additional_redirect_urls": [
      "http://localhost:3000/auth/callback",
      "https://myapp.com/auth/callback"
    ],
    "jwt_expiry": 3600,
    "enable_signup": true,
    "enable_anonymous_sign_ins": false
  },
  "db": {
    "seed": {
      "enabled": true,
      "sql_paths": [
        "./seeds/*.sql"
      ]
    }
  },
  "profiles": {
    "local": {
      "mode": "local",
      "workflow": "dashboard",
      "branches": [
        "feature/*",
        "fix/*",
        "dev"
      ]
    },
    "production": {
      "mode": "remote",
      "workflow": "git",
      "branches": [
        "main",
        "master"
      ]
    }
  },
  "workflow_profile": "staged"
}
